	<os:object-store name="Cart_Object_store" doc:name="Object store" doc:id="b4a4ffec-d934-485d-b317-bf5ba5c423e1" maxEntries="100" expirationInterval="60" config-ref="Cart_ObjectStore_Config" />
	

